
      #way-1
def voting(age):
    if(age>18):
        print("eligible for vote")
    else:
        print("not eligible for vote")
age=int(input())
voting(age)

       #way-2
def voting(age):
    if(age>18):
        return "eligible"
    else:
        return "not eligible"
age=int(input())
print(voting(age))

        #way-3
def voting(age):
    if(age>18):
        return True
    else:
        return False
age=int(input())
if(voting(age)):
    print("Yes")
else:
    print("No")
        
    
